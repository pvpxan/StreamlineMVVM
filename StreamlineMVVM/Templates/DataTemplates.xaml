<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:StreamlineMVVM">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/StreamlineMVVM;component/Templates/ButtonExtended.xaml" />
        <ResourceDictionary Source="pack://application:,,,/StreamlineMVVM;component/Templates/CheckBox.xaml" />
        <ResourceDictionary Source="pack://application:,,,/StreamlineMVVM;component/Templates/FlatComboBox.xaml" />
        <ResourceDictionary Source="pack://application:,,,/StreamlineMVVM;component/Templates/GroupBoxImproved.xaml" />
        <ResourceDictionary Source="pack://application:,,,/StreamlineMVVM;component/Templates/ListViewExtended.xaml" />
        <ResourceDictionary Source="pack://application:,,,/StreamlineMVVM;component/Templates/ScrollBarExtended.xaml" />

        <!-- START User defined custom dialog DataTypes -->
        <ResourceDictionary>
            <DataTemplate DataType="{x:Type local:WindowsMessageViewModel}">
                <local:WindowsMessage/>
            </DataTemplate>
        </ResourceDictionary>
        <!-- END User defined custom dialog DataTypes -->

    </ResourceDictionary.MergedDictionaries>

</ResourceDictionary>